{
    "name": "lucacri/nova-unlayerjs",
    "description": "UnlayerJs field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/lucacri/nova-unlayerjs",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Critelli",
            "email": "lucacri@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Lucacri\\NovaUnlayerjs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lucacri\\NovaUnlayerjs\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
